	background-color: WHITE;
	border: 1px solid #ccc;
	border-radius: 3px 3px 0 0;
	color: DimGray;
	border-width: 1px 1px 0 0;
	cursor: pointer;
}
.previewtab input:nth-of-type(1):checked ~ label:nth-of-type(1) {
	background-color: #fcfcfc;
	color: BLACK;
}

.previewtab input:nth-of-type(2):checked ~ label:nth-of-type(2) {
