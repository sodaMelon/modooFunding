body {
	width: 100%;
	height: 100%;
	background-color: white;
	margin: 0;
	padding: 0;
}

form#newproject {
	font-family: nanumsquareround;
	width: 100%;
	display: flex;
	justify-content: center;
	text-align: center;
}

form#newproject .preview {
	margin: 50px 50px 30px 30px;
	width: 40%;
	height: 700px;
	overflow-y: scroll;
}

form#newproject fieldset#content {
	border: none;
}

form#newproject fieldset#giftlist {
	border: none;
}

#gift1, #gift2, #gift3 {
	visibility: hidden;
	line-height: 150%;
	width: 500px;
	float: center;
	text-align: center;
	display: inline-block;
	padding: 10px 10px 10px 10px;
	color: dimgray;
}

form#newproject fieldset#content h1 {
	font-weight: normal;
	float: left;
	font-family: Montserrat;
	text-align: left;
	font-size: 12px;
	color: Silver;
}

.tabmenu {
	width: 700px;
	align-items: center;
}

.tabmenu label {
	display: inline-block;
	font-size: 14px;
	padding: 10px;
	text-align: center;
	width: 40%;
	line-height: 20px;
	background-color: WHITE;
	border: 1px solid #ccc;
	border-radius: 3px 3px 0 0;
	color: DimGray;
	border-width: 1px 1px 0 0;
	cursor: pointer;
}

.tabmenu input:nth-of-type(1):checked ~ label:nth-of-type(1) {
	background-color: #fcfcfc;
	color: BLACK;
}

.tabmenu input:nth-of-type(2):checked ~ label:nth-of-type(2) {
	background-color: #fcfcfc;
	color: BLACK;
}

.tabmenu>* {
	display: none;
}

.tabmenu input:nth-child(1):checked ~ .tab1_content, .tabmenu input:nth-child(2):checked 
	~ .tab2_content {
	display: block;
}

.tabmenu .tab1_content, .tabmenu .tab2_content {
	padding: 32px;
	border: 1px solid #ccc;
	background-color: white;
	height: 600px;
}

.tabmenu label#tab1:nth-of-type(1):hover {
	font-weight: bold;
	background-color: #fff9ee;
}

.tabmenu label#tab2:nth-of-type(2):hover {
	font-weight: bold;
	background-color: #fff9ee;
}

table#overview {
	align-items: center;
	display: flex;
	justify-content: center;
	width: 100%;
}

table#overview th {
	font-weight: normal;
