.previewtab input:nth-child(1):checked ~ .tab3_content, .previewtab input:nth-child(2):checked ~ .tab4_content, .previewtab input:nth-child(3):checked ~  .tab5_content {
	display: block;
}

.previewtab .tab3_content, .previewtab .tab4_content, .previewtab .tab5_content  {
	padding: 32px;
	background-color: white;
	height: 600px;
}

.previewtab label#tab3:nth-of-type(1):hover {
	font-weight: bold;
	background-color: #fff9ee;
}

.previewtab label#tab4:nth-of-type(2):hover {
	font-weight: bold;
	background-color: #fff9ee;
}

.previewtab label#tab5:nth-of-type(3):hover {
	font-weight: bold;
	background-color: #fff9ee;
}

.tooltip {
	position: relative;
	display: inline-block;
	font-weight: bold;
	color: red;
}

.text {
	visibility: hidden;
	width: 150px;
	background-color: #4e9ca9;
	color: white;
	text-align: center;
	border-radius: 6px;
	padding: 5px;
	position: absolute;
	top: 30px;
	margin-left: -60px;
	opacity: 0;
	transition: opacity 2s;
}

.text:after {
	content: "";
	border-width: 5px;
	border-style: solid;
	position: absolute;
	top: -10px;
	left: 50%;
	margin-left: -5px;
}

.tooltip:hover .text {
	visibility: visible;
	opacity: 1;
}

