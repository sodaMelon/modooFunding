package service;

public class BoardModCommand {

	String currentCsub;
	String newCsub;
	String currentCcon;
	String newCcon;
	public String getCurrentCsub() {
		return currentCsub;
	}
	public void setCurrentCsub(String currentCsub) {
		this.currentCsub = currentCsub;
	}
	public String getNewCsub() {
		return newCsub;
	}
	public void setNewCsub(String newCsub) {
		this.newCsub = newCsub;
	}
	public String getCurrentCcon() {
		return currentCcon;
	}
	public void setCurrentCcon(String currentCcon) {
		this.currentCcon = currentCcon;
	}
	public String getNewCcon() {
		return newCcon;
	}
	public void setNewCcon(String newCcon) {
		this.newCcon = newCcon;
	}
	
}
